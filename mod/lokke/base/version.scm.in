(define-module (lokke base version)
  #:export (major minor increment qualifier version))

(define major @LOKKE_MAJOR_VERSION@)
(define minor @LOKKE_MINOR_VERSION@)
(define increment @LOKKE_INCREMENT_VERSION@)
(define qualifier #nil)
(define (version) "@VERSION@")
