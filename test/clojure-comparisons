(ns test.clojure-comparisons
  (:require
   [clojure.test :refer [begin-tests end-tests is testing]]))

(begin-tests (ns-name *ns*))

(testing "="
 (is (= 1 1))
 (is (not (= 1 2)))
 (is (not (= 1 "x")))
 (is (= "x" "x"))
 (is (not (= "x" "y")))

 (is (not (= nil false)))
 (is (not (= nil ())))
 (is (not (= nil [])))

 (is (= [1 2 3] [1 2 3]))
 (is (not (= [1 2] [1 2 3])))

 (is (not (= () nil)))

 (is (= '([1]) '((1))))
 (is (= [[1]] ['(1)]))
 (is (= [1 [2 3 '(4 5 6)]]
        '(1 [2 3 [4 5 6]]))))

(end-tests (ns-name *ns*) :exit? true)
