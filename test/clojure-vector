(ns test.clojure-collection
  (:require
   [clojure.test :refer [begin-tests end-tests is testing]]))

(begin-tests (ns-name *ns*))

(is (vector? (vector)))
(is (not (vector? 'something)))
(is (counted? (vector)))
(is (= [1 2 3] [1 2 3]))
(is (= [1 2 3] (vector 1 2 3)))
(is (= [1 2 3] (vec '(1 2 3))))
(is (= [1 2 3] '(1 2 3)))
(is (= 2 (get [1 2 3] 1)))
(is (= 2 (nth [1 2 3] 1)))
(is (= [1 2 3] (conj [1 2] 3)))

(end-tests (ns-name *ns*) :exit? true)
