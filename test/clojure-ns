(ns test.clojure-ns
  (:require
   [clojure.string :as str])
  (:require
   [clojure.test :refer [begin-tests end-tests is testing]]))

(begin-tests (ns-name *ns*))

(is (= 'test.clojure-ns (ns-name *ns*)))
(is (= clojure.string/join str/join))

(end-tests (ns-name *ns*) :exit? true)
